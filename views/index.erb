<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet" />
    <link rel="stylesheet" href="/import.css" />
    <link rel="website icon" href="/yt.png" />
    <title>Youtube Clone</title>
  </head>
  <body>
      <header class="header">
        <div class="logo left">
          <i id="menu" class="material-icons">menu</i>
          <img src="https://www.freecodecamp.org/news/content/images/2022/01/yt-logo.png">
        </div>

        <div class="search center">
          <form action="">
            <input type="text" placeholder="Search" />
            <button><i class="material-icons">serach</i></button>
          </form>
          <i class="material-icons mic">mic</i>
        </div>

        <div class="icons right">
          <i class="material-icons">videocam</i>
          <i class="material-icons">apps</i>
          <i class="material-icons">notifications</i>
          <i class="material-icons display-this">account_circle</i>
        </div>
      </header>

      <main>
          <div class="side-bar">
            <div class="nav">
              <a class="nav-link active">
                <i class="material-icons">home</i>
                <span>Home</span>
              </a>
              <a class="nav-link">
                <i class="material-icons">local_fire_department</i>
                <span>Trending</span>
              </a>
              <a class="nav-link">
                <i class="material-icons">subscriptions</i>
                <span>Subscriptions</span>
              </a>
          </div>
            <hr>
              <a class="nav-link">
                <i class="material-icons">library_add_check</i>
                <span>Library</span>
              </a>
              <a class="nav-link">
                <i class="material-icons">history</i>
                <span>History</span>
              </a>
              <a class="nav-link">
                <i class="material-icons">play_arrow</i>
                <span>Your Videos</span>
              </a>
              <a class="nav-link">
                <i class="material-icons">watch_later</i>
                <span>Watch Later</span>
              </a>
              <a class="nav-link">
                <i class="material-icons">thumb_up</i>
                <span>Liked Videos</span>
              </a>
          </div>

          <div class="content">
            <div class="card-groups">
              <div classs="card">
                <iframe width="400" height="250" src="https://www.youtube.com/embed/dh6mAsPI8Bg?si=uk68E8nxCbNUZ3lM" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>

                <iframe width="400" height="250" src="https://www.youtube.com/embed/Zlu7NwOPLQQ?si=BepuDzDgE5upFl5E" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>

                <iframe width="400" height="250" src="https://www.youtube.com/embed/UKotAQooPtA?si=nFUGszeqdslpeUk6" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>

                <iframe width="400" height="250" src="https://www.youtube.com/embed/NNKx3NaBchA?si=XiDmT5Yivg-GdkDs" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>

                <iframe width="400" height="250" src="https://www.youtube.com/embed/fp9akpHTZeM?si=klwiuKJzvqDO-3eW" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>

                <iframe width="400" height="250" src="https://www.youtube.com/embed/NbfZbb6Vh-s?si=odrQkz1-2SR9CzHM" title="YouTube video player" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" allowfullscreen></iframe>
              </div>
            </div>
          </div>
      </main>
  </body>
</html>